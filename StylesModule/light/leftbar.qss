MenuBar {
    background: #F3F3F3;
}

MenuBar MenuBarMovePane {
    border: none;
    background: #F3F3F3;
}

QPushButton#BtnMinimize {
    background: #F3F3F3;
}

QPushButton#BtnMaximize {
    background: #F3F3F3;
}

QPushButton#BtnClose {
    background: #F3F3F3;
}

QPushButton#BtnMinimize:hover {
    background: #ececec;
}

QPushButton#BtnMaximize:hover {
    background: #ececec;
}

QPushButton#BtnClose:hover {
    background: #D61425;
    image: url(":/MenuBar/close-hover.svg");
}

LeftBar QFrame {
    background: #F3F3F3;
    min-width: 1;
}

LeftBar {
    background: #333333;
}

MenuBar {
    background: #F3F3F3;
}

MenuBar QWidget#Menus{
    background: #F3F3F3;
}

MenuBar QMenuBar::item {
    background: #F3F3F3;
}

MenuBar QMenuBar::item:selected {
    background: #ececec;
}

QWidget[errorSelection="true"]{
    background: #933C2B;
}

TreeBar QLabel#Title {
    color: #9d9d9d;
    font-size: 12px;
}

MenuBar QLabel#LogoIcon {
    background: #F3F3F3;
}

MenuBarMovePane QLabel#Title {
    background: #F3F3F3;
    color: #9d9d9d;
}

MenuBarMovePane QLabel#Icon {
    background: #F3F3F3;
}

WidgetsEditingObjectSaveCancelButtons {
    background: #F3F3F3;
}

QFrame[Footer="true"]
{
    background: #F3F3F3;
}

QFrame[Footer="true"] QLabel
{
    background: #F3F3F3;
}

QFrame[Footer="true"] QCheckBox
{
    background: #F3F3F3;
}

QFrame[Footer="true"] QDoubleSpinBox
{
    background: #F3F3F3;
}

QFrame[Footer="true"] QSpinBox
{
    background: #F3F3F3;
}

QTreeView#OffsetsTreeView {
    background: white;
}

TreeBar QFrame#Header {
    background: #f3f3f3;
    border-bottom: 1px solid #D2D2D2;
}

TreeBar QTreeView {
    background: #f3f3f3;
}

TreeBar QTreeView:hover {
    background: #f3f3f3;
}

TreeBar QLabel {    
    background: #f3f3f3;
    color: #9d9d9d;
}

MainWindow {
    qproperty-enabledTableColor: white;
    qproperty-enabledTableTextColor: black;
    qproperty-disabledTableColor: #d4d4d4;
    qproperty-disabledTableTextColor: black;
}
