LeftBar {
    background: #333333;
    padding: 20 0 0 0;
    min-width: 48;
}

LeftBar QFrame {
    background: white;
    min-width: 0;
    max-width: 0;
}

QTreeView {
    qproperty-indentation: 12;
}

MenuBar {
    min-height: 30;
}

MenuBar {
    background: #3C3C3C;
}

MenuBar MenuBarMovePane {
    border: none;
    background: #3C3C3C;
}

MenuBarMovePane QLabel#Title {
    background: #3C3C3C;
}

QPushButton#BtnMinimize {
    background: #3C3C3C;
}

QPushButton#BtnMaximize {
    background: #3C3C3C;
}

QPushButton#BtnClose {
    background: #3C3C3C;
}

QPushButton#BtnMinimize:hover {
    background: #505050;
}

QPushButton#BtnMaximize:hover {
    background: #505050;
}

QPushButton#BtnClose:hover {
    background: #D61425;
    image: url(":/menu_bar/close-hover.svg");
}

MenuBarMovePane {
    min-height: 28px;
    max-height: 28px;
}

MenuBarMovePane QPushButton{
    min-height: 15px;
}

MenuBar QMenuBar {
    background: #3C3C3C;
    color: #9d9d9d;
}

MenuBar QMenuBar::item:selected {
    background: #505050;
}

QPushButton#BtnMinimize {
    qproperty-text: "";
    image: url(":/menu_bar/minimize.svg");
    background: none;
    border-radius: none;
    border: none;
    padding: 1 5 12 5;
}

QPushButton#BtnMinimize:hover {
    border: none;
    border-radius: none;
}

QPushButton#BtnMaximize[windowMaximized="true"] {
    padding: 9 5 8 5;
    image: url(":/menu_bar/doublesquare.svg");
}

QPushButton#BtnMaximize[windowMaximized="false"] {
    image: url(":/menu_bar/square.svg");
}

QPushButton#BtnMaximize {
    qproperty-text: "";
    image: url(":/menu_bar/square.svg");
    border-radius: none;
    background: none;
    padding: 7 5 6 5;
    border: none;
}

QPushButton#BtnMaximize:hover {
    border: none;
    border-radius: none;
}

QPushButton#BtnClose {
    qproperty-text: "";
    image: url(":/menu_bar/close.svg");
    border-radius: none;
    background: none;
    padding: 7 5 6 5;
    border: none;
}

QPushButton#BtnClose:hover {
    border: none;
    border-radius: none;
}

TreeBar QFrame#Header {
    background: #252526;
    border: none;
    border-bottom: 1px solid #3C3C3C;
    border-radius: 0px;
    padding-right: 15px;
    padding-left: 15px;
}

TreeBar QLabel {    
    padding-top: 12px;
    padding-bottom: 11px;
    background: #252526;
    color: #9d9d9d;
}

MenuBarMovePane QLabel#Icon {
    min-width: 20px;
    padding: 5px;
    padding-left: 10px;
    padding-right: 10px;
    border: none;
    image: url(:/Logo.svg);
}

MenuBar MenuBarMovePane QLabel#Icon {
    min-width: 0px;
    max-width: 0px;
    padding: 0px;
    image: none;
}

MenuBar QLabel#LogoIcon {
    min-width: 20px;
    padding: 5px;
    padding-left: 10px;
    padding-right: 10px;
    border: none;
    image: url(:/Logo.svg);
}

QFrame#MainFooter {
    min-height: 22px;
    max-height: 22px;
    border-radius: 0px;
    border: none;
    padding: 0px 15px 0px 15px; 
}

DDFooter QMenuBar {
    font-size: 12px;
    background: #007acc;
}

DDFooter QMenu::item{
    font-size: 12px;
    background: #007acc;
}

DDFooter QMenu::item::selected{
    background: #1F8AD2;
}

DDFooter QMenuBar::item {
  color: white;
  background: transparent;
  padding: 5px 8px 2px 8px;
}

DDFooter QMenuBar::item::selected {
    background: #1F8AD2;
} 

DDFooter QMenuBar::item::pressed:disabled {
    background: #007acc;
} 
