}

DDFooter QLabel{
    font-size: 12px;
}

QFrame#MainFooter QLabel#FooterProgressBarLabel{
    font-size: 8px;
    qproperty-alignment: 'AlignVCenter | AlignLeft';
}

QTreeView#OffsetsTreeView {
    background: #1e1e1e;
}

TreeBar QFrame#Header {
    background: #252526;
    border-bottom: 1px solid #3C3C3C;
}

TreeBar QLabel {    
