}

WidgetsStyleSettings {
    qproperty-enabledTableColor: #252526;
    qproperty-enabledTableTextColor: white;
    qproperty-disabledTableColor: rgba(255, 255, 255, 16);
    qproperty-disabledTableTextColor: rgb(153, 162, 173);
}

QPushButton[DrawerControl="true"] {
    min-height: 20px;
